<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="DarkColor">#5C5C5C</Color>
    <Color x:Key="DarkBackgroundColor">#2B2B2D</Color>
    <Color x:Key="LightForegroundColor">#808080</Color>
    <Color x:Key="WhiteColor">#ffffff</Color>

    <SolidColorBrush x:Key="WhiteColorForeground" Color="{StaticResource WhiteColor}"/>
    <SolidColorBrush x:Key="WhiteThemes" Color="{StaticResource WhiteColor}"/>

    <SolidColorBrush x:Key="DarkThemes" Color="{StaticResource DarkColor}"/>
    <SolidColorBrush x:Key="ForLightThemesForeground" Color="{StaticResource LightForegroundColor}"/>
    <SolidColorBrush x:Key="DarkBackgroundThemes" Color="{StaticResource DarkBackgroundColor}"/>
</ResourceDictionary>
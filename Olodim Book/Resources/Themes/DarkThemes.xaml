<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBorder" Color="#2B2B2D"/>
    <SolidColorBrush x:Key="BorderBrush" Color="#5C5C5C"/>
    <SolidColorBrush x:Key="ForegroundTextBlock" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="BackgroundRadioButton" Color="#5C5C5C"/>

    <SolidColorBrush x:Key="ButtonLightBackground" Color="#5C5C5C"/>
    <SolidColorBrush x:Key="ButtonDarkBackground" Color="#2B2B2D"/>

    <LinearGradientBrush x:Key="KeyBoardButtonBackground"
                         StartPoint="0 1"
                         EndPoint="1 0">
        <GradientStop Color="#5C5C5C" Offset="0"/>
        <GradientStop Color="#2B2B2D" Offset="100"/>
    </LinearGradientBrush>


</ResourceDictionary> 
